{
  "name": "prisma-webpack-plugin",
  "version": "0.1.1",
  "description": "use prisma to migrate database when webpack working",
  "keywords": [
    "webpack",
    "prisma"
  ],
  "author": "Dylan Li <xiaoboleee@gmail.com>",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "package.json",
    "esm",
    "lib",
    "types",
    "!**/*.test.js",
    "!**/*.test.js.map",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts.map"
  ],
  "homepage": "https://github.com/dylan-kitn/prisma-webpack-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylan-kitn/prisma-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/dylan-kitn/prisma-webpack-plugin/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true mocha",
    "lint": "eslint src",
    "tsc": "tsc",
    "start": "cross-env TS_NODE_FILES=true mocha --watch",
    "build:esm": "tsc --module ESNext --outDir esm",
    "build:lib": "tsc --module CommonJS --outDir lib",
    "build:types": "tsc --declaration --declarationMap --emitDeclarationOnly --outDir types",
    "build": "npm run build:types && npm run build:lib && npm run build:esm",
    "clear111": "del esm lib types",
    "prepublish111": "npm run clear && npm run build"
  },
  "dependencies": {
    "@prisma/engines": "^5.5.2",
    "@prisma/internals": "^5.5.2",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mocha": "^10.2.0",
    "sinon": "^17.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "@prisma/client": "^5.5.2",
    "webpack": "^4.37.0 || ^5.0.0"
  }
}
